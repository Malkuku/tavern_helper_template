{
  "rules": {
    "基础算术测试": {
      "enable": true,
      "path": "*",
      "order": 0,
      "handle": {
        "执行计算": {
          "order": 0,
          "op": "<<op> $[角色.星宫诗羽.特殊状态.好感度] #[=] &[{num}100] #[+] &[{num}50] #[-] &[{num}25] #[*] &[{num}2] #[/] &[{num}5] >"
        }
      }
    }
  }
}
