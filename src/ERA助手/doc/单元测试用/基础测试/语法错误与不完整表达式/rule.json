{
  "rules": {
    "语法错误测试": {
      "enable": true,
      "path": "*",
      "order": 0,
      "handle": {
        "单目函数缺少括号": {
          "order": 0,
          "op": "<<op> $[角色.夜月梦.特殊状态.好感度] #[=] #[{floor} $[角色.星宫诗羽.特殊状态.好感度] #[+] $[角色.哈基米.特殊状态.好感度]] >"
        },
        "错误的操作符": {
          "order": 1,
          "op": "<<op> $[世界.地点] #[=] &[{num}1] #[$] &[{num}2] >"
        },
        "不完整的IF": {
          "order": 2,
          "if": "<<if> $[角色.星宫诗羽.已出场] ?[==] >",
          "op": "<<op> $[世界.地点] #[=] &[{str}不完整的IF] >"
        }
      }
    }
  }
}
