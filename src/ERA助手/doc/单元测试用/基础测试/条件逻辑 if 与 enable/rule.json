{
  "rules": {
    "规则级IF为假": {
      "enable": true,
      "path": "*",
      "order": 0,
      "if": "<<if> $[角色.夜月梦.特殊状态.好感度] ?[<] &[{num}500] >",
      "handle": {
        "不应执行的操作": {
          "order": 0,
          "op": "<<op> $[世界.地点] #[=] &[{str}规则级IF为假] >"
        }
      }
    },
    "处理项级IF": {
      "enable": true,
      "path": "*",
      "order": 1,
      "handle": {
        "此项应执行": {
          "order": 0,
          "if": "<<if> $[角色.夜月梦.特殊状态.好感度] ?[>=] &[{num}600] >",
          "op": "<<op> $[世界.时间] #[=] &[{str}傍晚] >"
        },
        "此项不应执行": {
          "order": 1,
          "if": "<<if> $[角色.星宫诗羽.特殊状态.好感度] ?[>] &[{num}10] >",
          "op": "<<op> $[世界.地点] #[=] &[{str}处理项级IF为假] >"
        }
      }
    },
    "被禁用的规则": {
      "enable": false,
      "path": "*",
      "order": 2,
      "handle": {
        "不应执行的操作": {
          "order": 0,
          "op": "<<op> $[世界.当前人物] #[=] &[{str}任何人] >"
        }
      }
    }
  }
}
