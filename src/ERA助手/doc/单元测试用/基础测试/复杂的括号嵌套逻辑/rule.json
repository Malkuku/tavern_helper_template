{
  "rules": {
    "复杂括号嵌套计算": {
      "enable": true,
      "path": "*",
      "order": 0,
      "handle": {
        "计算世界熵值": {
          "order": 0,
          "op": "<<op> $[世界.时间] #[=] #[{floor}( ( ( ($[角色.星宫诗羽.特殊状态.好感度] #[+] &[{num}10]) #[*] &[{num}2] ) #[-] ($[角色.白石䌷.特殊状态.好感度] #[/] &[{num}2]) ) #[*] ( ($[角色.橘瑠奈.特殊状态.好感度] #[+] &[{num}5]) #[%] &[{num}3] ) )] >"
        }
      }
    }
  }
}
